{
  "version": "0.2.0",
  "configurations": [{
      "name": "Launch backend",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/app.js",
      "cwd": "${workspaceRoot}",
      "preLaunchTask": null,
      "console": "internalConsole",
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/**/*.js"]
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/frontend/build/",
      "diagnosticLogging": true,
      "sourceMapPathOverrides": {"/source/*": "${workspaceRoot}/*"}
    }
  ]
}
